<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
    <key>UserName</key>
    <string>martin.dagostino</string>
    <key>Label</key>
    <string>org.babel.agent</string>
    <key>ProgramArguments</key>
    <array>
        <string>/Users/{USER}/go/bin/babel-agent</string>
        <string>--config</string>
        <string>/Users/{USER}/.babel/babel.toml</string>
    </array>
    <key>RunAtLoad</key>
    <true/>
    <key>KeepAlive</key>
    <true/>
    <key>StandardOutPath</key>
    <string>/Users/{USER}/.babel/babel-agent.log</string>
    <key>StandardErrorPath</key>
    <string>/Users/{USER}/.babel/error-agent.log</string>
</dict>
</plist>